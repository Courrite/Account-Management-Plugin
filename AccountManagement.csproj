<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="OTAPI" Version="2.0.0.50" />
    <PackageReference Include="TSAPI" Version="5.2.0" />
    <PackageReference Include="TShock" Version="5.2.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="MongoDB.Driver">
      <HintPath>libs\MongoDB.Driver.dll</HintPath>
    </Reference>
    <Reference Include="MongoDB.Bson">
      <HintPath>libs\MongoDB.Bson.dll</HintPath>
    </Reference>
    <Reference Include="Tomlyn">
      <HintPath>libs\Tomlyn.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
